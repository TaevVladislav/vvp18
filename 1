#include <iostream>
using namespace std;
int main() {
	int n = 6; int r = 0;
	int a[6] = {9, 4, 1, -9, 0, 2};
	int b[6] = { 8, 7, 11, -5, 4, 5 };
	for (int j = 0; j < n; j++) {
		r = a[j];
		a[j] = b[j];
		b[j] = r;
	}
	cout << "A = {";
	for (int j = 0; j < n; j++) {
		cout << a[j] << ", ";
	}
	cout << "}" << endl;
	cout << "B = {";
	for (int j = 0; j < n; j++) {
		cout << b[j] << ", ";
	}
	cout << "}";
}
